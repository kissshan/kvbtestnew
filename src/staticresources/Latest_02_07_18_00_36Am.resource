 <field id=\"genesis__Application__c\"/>\n        <field id=\"genesis__Application__r.Name\"/>\n        <field id=\"clcommon__Account__c\"/>\n        <field id=\"clcommon__Account__r.Name\"/>\n        <field id=\"clcommon__Type__c\"/>\n        <field id=\"clcommon__Type__r.Name\"/>\n        <field id=\"clcommon__Signer_Capacity__c\"/>\n        <field id=\"clcommon__Signing_on_Behalf_of__c\"/>\n        <field id=\"clcommon__Signing_on_Behalf_of__r.Name\"/>\n      </fields>\n      <conditions>\n        <condition type=\"param\" value=\"id\" field=\"Id\" operator=\"=\" enclosevalueinquotes=\"true\" novaluebehavior=\"noquery\"/>\n      </conditions>\n      <actions/>\n    </model>\n    <model id=\"RelatedAppWithSameParty\" limit=\"\" query=\"true\" createrowifnonefound=\"false\" adapter=\"salesforce\" type=\"\" sobject=\"clcommon__Party__c\">\n      <fields>\n        <field id=\"genesis__Application__c\"/>\n        <field id=\"genesis__Application__r.Name\"/>\n      </fields>\n      <conditions>\n        <condition type=\"modelmerge\" value=\"\" field=\"genesis__Application__r.genesis__Parent_Application__c\" operator=\"=\" novaluebehavior=\"noquery\" model=\"SelectedParentParty\" enclosevalueinquotes=\"true\" mergefield=\"genesis__Application__c\"/>\n        <condition type=\"modelmerge\" value=\"\" field=\"clcommon__Account__c\" operator=\"=\" model=\"SelectedParentParty\" enclosevalueinquotes=\"true\" mergefield=\"clcommon__Account__c\" novaluebehavior=\"noquery\"/>\n        <condition type=\"modelmerge\" value=\"\" field=\"clcommon__Type__c\" operator=\"=\" model=\"SelectedParentParty\" enclosevalueinquotes=\"true\" mergefield=\"clcommon__Type__c\" novaluebehavior=\"noquery\"/>\n      </conditions>\n      <actions/>\n    </model>\n    <model id=\"RemainingApplications\" limit=\"\" query=\"true\" createrowifnonefound=\"false\" adapter=\"salesforce\" type=\"\" sobject=\"genesis__Applications__c\">\n      <fields>\n        <field id=\"Name\"/>\n        <field id=\"Id\"/>\n        <field id=\"genesis__CL_Product__c\"/>\n        <field id=\"genesis__CL_Product__r.Name\"/>\n        <field id=\"genesis__CL_Product__r.clcommon__Product_Name__c\"/>\n      </fields>\n      <conditions>\n        <condition type=\"modelmerge\" value=\"\" field=\"genesis__Parent_Application__c\" operator=\"=\" model=\"SelectedParentParty\" enclosevalueinquotes=\"true\" mergefield=\"genesis__Application__c\" novaluebehavior=\"noquery\"/>\n        <condition type=\"modelmerge\" value=\"\" field=\"Id\" operator=\"not in\" model=\"RelatedAppWithSameParty\" enclosevalueinquotes=\"true\" mergefield=\"genesis__Application__c\" novaluebehavior=\"deactivate\"/>\n      </conditions>\n      <actions/>\n    </model>\n    <model id=\"NewlyCreatedPartyModel\" limit=\"\" query=\"false\" createrowifnonefound=\"false\" adapter=\"salesforce\" type=\"\" sobject=\"clcommon__Party__c\">\n      <fields>\n        <field id=\"clcommon__Account__c\"/>\n        <field id=\"clcommon__Account__r.Name\"/>\n        <field id=\"genesis__Application__c\"/>\n        <field id=\"genesis__Application__r.Name\"/>\n        <field id=\"clcommon__Signer_Capacity__c\"/>\n        <field id=\"clcommon__Signing_on_Behalf_of__c\"/>\n        <field id=\"clcommon__Signing_on_Behalf_of__r.Name\"/>\n        <field id=\"clcommon__Type__c\"/>\n        <field id=\"clcommon__Type__r.Name\"/>\n      </fields>\n      <conditions/>\n      <actions/>\n    </model>\n  </models>\n  <components>\n    <grid uniqueid=\"sk-3Iq_ma-315\">\n      <divisions>\n        <division behavior=\"flex\" minwidth=\"100px\" ratio=\"1\" verticalalign=\"top\">\n          <components>\n            <pagetitle model=\"SelectedParentParty\" uniqueid=\"addPartyErrorTitle\">\n              <actions/>\n            </pagetitle>\n            <skootable showconditions=\"true\" showsavecancel=\"false\" showerrorsinline=\"true\" searchmethod=\"server\" searchbox=\"true\" showexportbuttons=\"false\" pagesize=\"10\" createrecords=\"false\" model=