on_20_deone_change__c===true){\n                    //appRow.Application_Stage__c = 'Application Filling - Final';\n                    appStage = 'Final Sanction';\n                    //appRow.Critical_Change_done__c = true;\n                }else{\n                    appStage = 'Application Filling - Final';\n\t\t\t\t\tuiStage = 'CompanyDetailsView';\n                }\n            }else if(appRow.Application_Stage__c ===  'Application review-Critical changes'){\n\t\t\t\tif(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Financials_upload_check_done__c===true &amp;&amp; appRow.Key_person_change_done__c===true &amp;&amp; appRow.Section_20_deone_change__c===true){\n\t\t\t\t\t//appRow.Application_Stage__c = 'Application Filling - Critical Changes';\n\t\t\t\t\tappStage = 'Application review -with financials';\n\t\t\t\t}else{\n\t\t\t\t\tappStage = 'Application filling initiated';\n\t\t\t\t\tuiStage = 'CompanyDetailsView';\n\t\t\t\t}\n            }\n            isCritalChange = true;\n          }    \n      }else{\n          if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Financials_upload_check_done__c===true &amp;&amp; appRow.Key_person_change_done__c===true &amp;&amp; appRow.Section_20_deone_change__c===true){\n                appRow.Application_Stage__c = '';\n          }else if(appRow.Constitution_change_done__c===false &amp;&amp; appRow.Financials_upload_check_done__c===true &amp;&amp; appRow.Key_person_change_done__c===true &amp;&amp; appRow.Section_20_deone_change__c===true){\n                \n                //appRow.Sub_Stage__c = 'Constitution change decline';\n                subStage = 'Constitution change decline';\n                //appRow.Critical_Change_done__c = true;\n          }else if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Financials_upload_check_done__c===false &amp;&amp; appRow.Key_person_change_done__c===true &amp;&amp; appRow.Section_20_deone_change__c===true){\n                //appRow.Sub_Stage__c = 'Re-upload financials';\n                subStage = 'Re-upload financials';\n                //appRow.Critical_Change_done__c = true;\n          }else if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Financials_upload_check_done__c===true &amp;&amp; appRow.Key_person_change_done__c===false &amp;&amp; appRow.Section_20_deone_change__c===true){\n                //appRow.Sub_Stage__c = 'Key person change decline';\n                subStage = 'Key person change decline';\n                //appRow.Critical_Change_done__c = true;\n          }else if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Financials_upload_check_done__c===true &amp;&amp; appRow.Key_person_change_done__c===true &amp;&amp; appRow.Section_20_deone_change__c===false){\n                \n                //appRow.Sub_Stage__c = 'Section 20 change decline';\n                subStage = 'Section 20 change decline';\n                //appRow.Critical_Change_done__c = true;\n          }\n          \n      }\n      \n    }else if(appRow.RecordType.DeveloperName==='SME_Enhancement') {\n        \n        if(appRow.Application_Stage__c ===  'Application review-Critical changes'){\n           if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Key_person_change_done__c===true &amp;&amp; appRow.Section_20_deone_change__c===true){\n                //appRow.Application_Stage__c = 'Enhancement- Application submitted';\n                appStage = 'Enhancement- Application submitted';\n                bmTaskReq   = true;\n                isCritalChange = true;\n          }else if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Financials_upload_check_done__c===true &amp;&amp; appRow.Key_person_change_done__c===false &amp;&amp; appRow.Section_20_deone_change__c===true){\n               appStage = 'Enhancement- Application created';\n               subStage = 'Key person change decline';\n               uiStage  = 'KeyPersonDetails';\n          }\n          else{\n                //appRow.Application_Stage__c = 'Enhancement- Application created';\n                appStage = 'Enhancement- Application created';\n                //appRow.Application_UI_Stage__c = 'CompanyDetailsView';\n                uiStage = 'CompanyDetailsView';\n                subStage = 'More than 1 critical change decline';\n          }\n        }\n    }else if(appRow.RecordType.DeveloperName==='SME_Exceeding') {\n        \n        if(appRow.Application_Stage__c ===  'Application review-Critical changes'){\n           if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Key_person_change_done__c===true &amp;&amp; appRow.Section_20_deone_change__c===true){\n                //appRow.Application_Stage__c = 'Exceeding- Application submitted';\n                appStage = 'Exceeding- Application submitted';\n                bmTaskReq   = true;\n                isCritalChange = true;\n          }else if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Financials_upload_check_done__c===true &amp;&amp; appRow.Key_person_change_done__c===false &amp;&amp; appRow.Section_20_deone_change__c===true){\n               appStage = 'Exceeding- Application created';\n               subStage = 'Key person change decline';\n               uiStage  = 'KeyPersonDetails';                \n          }else{\n                //appRow.Application_Stage__c = 'Exceeding- Application created';\n                appStage = 'Exceeding- Application created';\n                //appRow.Application_UI_Stage__c = 'CompanyDetailsView';\n                uiStage = 'CompanyDetailsView';\n                subStage = 'More than 1 critical change decline';\n          }\n        }\n      \n   }else if(appRow.RecordType.DeveloperName==='SME_AdHoc') {\n       \n       if(appRow.Application_Stage__c ===  'Application review-Critical changes'){\n           if(appRow.Constitution_change_done__c===true &amp;&amp; appRow.Key_person_change_done__c===true &amp;&amp; appRow.Section_20_deone_c